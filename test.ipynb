    "from reco_plugin.constants import *\n",
    "\n",
    "def keVtoLambda(keV: float) -> float:\n",
    "    E = keV * KEV_TO_JOULE              # en J\n",
    "    wavelength_m = (PLANCK_J_S * LIGHT_SPEED) / E  # en m\n",
    "    return wavelength_m\n",
    "\n",
    "print(keVtoLambda(8))  # 1 keV to m"
   ]
  }
